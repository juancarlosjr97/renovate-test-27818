{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:all"
    ],
    "packageRules": [
        {
            "matchPackagePatterns": "*",
            "commitMessagePrefix": "{{#if (includes depTypeList 'dependencies')}}production{{else}}notProduction{{/if}}"
        }
    ],
    "fetchChangeLogs": "off"
}
